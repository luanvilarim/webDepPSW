<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
         xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <display-name>WebDep</display-name>
  
  
    <jsp-config>
        <jsp-property-group>
            <url-pattern>*.jsp</url-pattern>
            <el-ignored>false</el-ignored>
        </jsp-property-group>
    </jsp-config>
  
    <servlet>
        <servlet-name>cadastroSistema</servlet-name>
        <servlet-class>com.cefet.rj.webdep.controller.CadastroDeSistema</servlet-class>
    </servlet>


    <servlet-mapping>
        <servlet-name>cadastroSistema</servlet-name>
        <url-pattern>/cadastrodesistema.jsp</url-pattern>
    </servlet-mapping>
</web-app>
